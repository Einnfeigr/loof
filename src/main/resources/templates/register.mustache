{{#error}}
	<div class="error">
		<i>{{error}}</i>
	</div>
	<br>
{{/error}}
{{#id}}
	<form method="POST" >
		<input type="hidden" name="id" value="{{id}}" />
		<span><img src="/img/icon/005-id card.svg" />Iм'я</span>
		<input type="text" name="name" value="" />
		<span><img src="/img/icon/019-user.svg" />Логін</span>
		<input type="text" name="login" value=""/>
		<span><img src="/img/icon/email.svg" />Пошта</span>
		<input type="email" name="email" />
		<span><img src="/img/icon/009-lock.svg" />Пароль</span>
		<input type="password" name="password" value="" />
		<div class="buttons">
			<input name="submit" type="submit" value="Підтвердити" />
		</div>
	</form>
{{/id}}
{{^id}}
	<form id="code" method="GET" action="/register">
	</form>
	<table>
		<tr>
			<td>Введіть id щоб продовжити</td>
		</tr>
		<tr>
			<td><input form="code" type="text" name="code" placeholder="Код довжиною 8 символів"/></td>
		</tr>
		<tr>
			<td><input form="code" type="submit" value="Перевірити"/></td>
		</tr>
	</table>
{{/id}}
