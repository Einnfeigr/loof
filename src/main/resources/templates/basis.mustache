<!DOCTYPE html>
<html lang="ru">
	<head>
		{{>misc/elements/head}}
	</head>
	<body class="preload">
		<div class="body">
			{{#includePreloader}}
				{{>misc/elements/preloader}}
			{{/includePreloader}}
			<div class="page {{#pageName}}{{pageName}}{{/pageName}}">
				{{^excludeHeader}}
					{{>misc/elements/header}}
				{{/excludeHeader}}
				<div class="pageNav">
					{{#pageTitle}}
						<h1>{{pageTitle}}</h1>
					{{/pageTitle}}
				</div>
				<div class="content">
					{{{page}}}
				</div>
				{{^excludeFooter}}
					{{>misc/elements/footer}}
				{{/excludeFooter}}
			</div>
		</div>
	</body>
</html>